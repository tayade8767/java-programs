class Jay
{
    int roll;
    String name;
}

public class Akash
{
    public static void main(String args[])
    {
        Jay s1=new Jay();
        Jay s2=new Jay();
        s1.roll=64;
        s1.name="akash";                                                         ///  assign the value to the object of the variable
        s2.roll=61;
        s2.name="pushpa";
        System.out.println(s1.roll+" "+s1.name);
        System.out.println(s2.roll+" "+s2.name);
    }
}
