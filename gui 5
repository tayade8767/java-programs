

import java.awt.*;

import javax.swing.JFrame;
import javax.swing.JLabel;

class Myframe extends JFrame
{
     public Myframe()
     {
        JLabel l=new JLabel("Hello world");
        add(l);
        setVisible(true);
        setSize(200,300);
     }
}



public class Framework4 
{
    public static void main(String args[])
    {
          Myframe mf=new Myframe();
    }
}
