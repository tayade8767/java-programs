class Window                                                          //  parent class 
{
    Window()
    {
        System.out.println("it is the parent class constructor");
    }
}
class Glass extends Window                   //   it is the inheritance (IS-A relation)
{
    Glass()
    {
        super();                //calling to the parent class constructor  using the super() keyword || it must be 1st position while calling to parent class constructor
        System.out.println("it is the child class constructor");
    }
}
public class Akash
{
    public static void main(String args[])
    {
        Glass g=new Glass();                           // creating the object of the class Glass and calling to the constructor
    }
}
