public class Encapsulation1 
{
    String name;
    int age;
     void set(String name,int age)
    {
        this.name=name;
        this.age=age;
    }
    String  get()
    {
            return name;
    }
    int  get1()
    {
            return age;
    }
    public static void main(String args[])
    {
        Encapsulation1 e=new Encapsulation1();        
        e.set("Akash", 20);   
        System.out.println("name of the person="+e.get()); 
        System.out.println("age of the person="+e.get1()); 
    }
}
