public class Akash
{
    public static void main(String args[])
    {
        String a[]=new String[3];                                   // create the dynamically  array (in the string)
        a[0]="akash";
        a[1]="tejas";
        a[2]="punam";
        for(int i=0;i<3;i++)
        {
            System.out.println(a[i]);
        }
    }
}
