public class Akash
{
        static int n1=0,n2=1,n3,i,num=0;
        static void fibo(int count)
        {
            if(count>0)
            {
            n3=n1+n2;
            n1=n2;
            n2=n3;
            System.out.print(" "+n3);
            fibo(count-1);
            }
        }
    public static void main(String args[])
    {
        System.out.print(n1+" "+n2);
        fibo(num);
    }
}
