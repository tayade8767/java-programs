 
 
                                      //   it is the IPCASTING means the runtime polymorphism
 
 
 
 
 
 class Parent
{
    void run()
    {
        System.out.println("it is parent class");
    }
}
public class Child extends Parent
{
    void run()
    {
        System.out.println("it is Child class");
    }
    public static void main(String args[])
    {
        Parent p=new Child();                              //   in UPCASTING we can take the parent class object and taking the Child() class
        p.run();
    }
}
